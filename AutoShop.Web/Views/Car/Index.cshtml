@model IEnumerable<CarViewModel> 

<h1>Список авто</h1>
<a asp-action="Create" class="btn btn-success">Додати авто</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Марка</th>
            <th scope="col">Модель</th>
            <th scope="col">Рік</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Mark</td>
                <td>@item.Model</td>
                <td>@item.Year</td>
            </tr>
        }
    </tbody>
</table>